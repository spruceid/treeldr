use <http://www.w3.org/1999/02/22-rdf-syntax-ns#> as rdf;
use <http://www.w3.org/2000/01/rdf-schema#> as rdfs;
use <http://www.example.com/> as ex;

type Foo {
	rdf:type as "@type": required multiple &rdfs:Class,
	ex:a: Bar
}

type Bar {
	rdf:type as "@type": required multiple &rdfs:Class,
	ex:b: &rdfs:Resource
}

