@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix tldr: <https://treeldr.org/> .
@prefix ex: <https://example.com/> .

ex:type a rdfs:Datatype ;
	owl:onDatatype xsd:integer ;
	owl:withRestrictions (
		[ a owl:DatatypeRestriction ; xsd:minInclusive "12"^^xsd:integer ]
		[ a owl:DatatypeRestriction ; xsd:maxInclusive "120"^^xsd:integer ]
	) .

ex:layout a tldr:Layout ;
	tldr:name "Layout" ;
	tldr:derivedFrom tldr:Integer ;
	tldr:withRestrictions (
		[ a tldr:LayoutRestriction ; tldr:inclusiveMinimum "12"^^xsd:integer ]
		[ a tldr:LayoutRestriction ; tldr:inclusiveMaximum "120"^^xsd:integer ]
	) .