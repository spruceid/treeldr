use <https://treeldr.org/> as tldr;

type URL with tldr:URL;

/// Certificate or deed issued by an educational institution.
type Diploma {
	department: &Department,
	degree: Degree,
	year: Year,
	honors: Honors,
	url: URL
}

/// Department
type Department {
	parent: Department
}

type Degree;

type Year;

type Honors;