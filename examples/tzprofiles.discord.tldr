base <https://tzprofiles.com/>;

use <https://schema.org/> as schema;
use <https://tzprofiles.com/> as tz;

type DiscordChannelId;
type DiscordMessageId;

property discordChannelId: DiscordChannelId;
property discordMessageId: DiscordMessageId;

type DiscordVerification {
	schema:url
}

type DiscordVerificationMessage {
	tz:handle,
	tz:timestamp,
	tz:discordChannelId as channelId,
	tz:discordMessageId as messageId
}