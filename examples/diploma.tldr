base <https://example.com>;

use <http://www.w3.org/2001/XMLSchema#> as xs;

/// Certificate or deed issued by an educational institution.
type Diploma {
	/// Bar
	department: &Department,
	degree: Degree,
	year: Year,
	honors: Honors,
	url: xs:anyURI
}

type Foo {
	department: &Department,
}

/// Foo
type Department {
	parent: Department
}

type Degree;

type Year;

type Honors;